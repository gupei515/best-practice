{
    "name": "test",
    "version": "1.0.0",
    "description": "test",
    "author": "gupei515@gmail.com",
    "repository": ".../browse",
    "license": "SEE LICENSE IN LICENSE.txt",
    "scripts": {
        "clean": "rimraf build && rimraf dist",
        "start": "webpack serve --mode=development",
        "build": "npm run clean && webpack --mode=production",
        "build-dev": "npm run clean && webpack --mode=development",
        "lint": "eslint src/**/*.{ts,tsx}",
        "lint-fix": "npm run lint -- --fix",
        "_prettier": "prettier src/**/*.{ts,tsx}",
        "prettier": "npm run _prettier -- --check",
        "prettier-fix": "npm run _prettier -- --write",
        "prettylint": "npm run prettier && npm run lint",
        "prettylint-fix": "npm run prettier-fix && npm run lint-fix",
        "test": "jest --verbose",
        "test-watch": "jest --watch",
        "full-test": "npm run prettylint && npm run test",
        "coverage": "jest --coverage",
        "ci": "npm run prettylint && jest --coverage --maxWorkers=2",
        "cy:open": "cypress open",
        "cy:run": "cypress run",
        "cy:start": "cross-env NODE_ENV=test npm run start",
        "cy:start-and-open": "cross-env NODE_ENV=test start-test 9000 cypress open",
        "cy:run:jules": "cross-env CODE_COVERAGE=true cypress run --config-file cypress.jules.json",
        "cy:ci": "cross-env NODE_ENV=test start-test 9000 cy:run:jules"
    },
    "engines": {
        "node": "^12",
        "npm": "^6"
    },
    "lint-staged": {
        "*.{,ts,tsx}": [
            "prettier --write",
            "eslint --fix",
            "jest --bail --findRelatedTests"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged -r --no-stash",
            "pre-push": "npm run prettylint && npm run test"
        }
    },
    "dependencies": {
        "@testing-library/jest-dom": "5.11.9",
