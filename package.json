        "@cypress/code-coverage": "^3.9.11",
        "@istanbuljs/nyc-config-typescript": "^1.0.1",
        "@testing-library/jest-dom": "5.11.9",
        "@testing-library/react": "11.2.3",
        "@types/dagre": "^0.7.46",
        "@types/deep-diff": "0.0.30",
